   "source": [
    "''' Write a function to compute 5/0 and use try/except to catch the exceptions.'''"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "def div(a,b):\n",
    "    try:\n",
    "        x=a/b\n",
    "    except ZeroDivisionError:\n",
    "        print(\"error dividing by zero\")\n",
    "    else:\n",
    "        print(x)"
   ]
  },
  {
